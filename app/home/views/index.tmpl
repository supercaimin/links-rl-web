    <div class="content">
        <div class="content-header">
            <p class="content-title">{{ t "layout.home.hp" }}</p>
        </div>
        <div class="content-body">
            <div class="hot-card-row row">
                {{range .Propertys3}}
                <div class="col-md-4">
                    <div class="card hot-card ">
                        <div class="card-img-top">
                            <img src="{{.MainImage.URL}}">
                            <div class="card-mask">
                                <div class="card-mask-content">
                                    <dl>
                                        <dt>Saleable :</dt>
                                        <dd>{{.SaleableArea}} s.f.</dd>
                                    </dl>
                                    <dl>
                                        <dt>Outdoor :</dt>
                                        <dd>{{.OutdoorArea}} s.f.</dd>
                                    </dl>
                                    <dl>
                                        <dt>Gross :</dt>
                                        <dd>{{.GrossArea}}  s.f.</dd>
                                    </dl>
                                    <dl>
                                        <dt>Rooms :</dt>
                                        <dd>{{.NoOfBedRooms}}</dd>
                                    </dl>
                                </div>
                            </div>
                        </div>
                        <div class="card-body">
                            <h5 class="card-title">{{.BuildingName}} - {{.Floor}}-{{.Unit}}</h5>
                            <p class="card-text">For {{.For}} HK${{.SellingPrice}} incl.</p>
                            <a href="/show?id={{.ID}}" class="card-link">
                                <span class="jiantou jiantou-hui"></span>
                            </a>
                        </div>
                    </div>
                </div>
                {{end}}
            </div>

            <div class="live-in-hk text-center">
                <a href="javascript:"><img src="/assets/img/contact.png" alt=""></a>
            </div>

            <div class="contact container collapse">
                <form>
                    <div class="form-row">
                        <div class="form-group col-md-6 contact-name">
                            <input type="text" class="form-control" placeholder="Property Name">
                        </div>
                        <div class="form-group col-md-6 contact-address">
                            <input type="text" class="form-control" placeholder="Property Address">
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-6 contact-uname">
                            <input type="text" class="form-control" placeholder="Your Name">
                        </div>
                        <div class="form-group col-md-6 contact-phone">
                            <input type="text" class="form-control" placeholder="Contact">
                        </div>
                    </div>
                    <div class="form-group contact-text">
                        <textarea class="form-control" name="" rows="4"
                                  placeholder="Brief specifications of your property..."></textarea>
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </form>
            </div>
        </div>
    </div>
