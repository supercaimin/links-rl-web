                <p class="banner-title">{{ t "layout.home.search_tip" }}</p>
                <div class="banner-input-group">
                    <input class="banner-input" type="text" placeholder="{{ t "layout.home.search_placeholder" }}">
                </div>
                <div class="banner-btn-group">
                    <a class="btn" href="/buy">{{ t "layout.home.buy_a_property" }} &nbsp;&nbsp;&nbsp;-></a>
                    <a class="btn" href="/rent">{{ t "layout.home.rent_a_property" }} &nbsp;&nbsp;&nbsp; -></a>
                </div>
            </div>
        </div>
    </div>
